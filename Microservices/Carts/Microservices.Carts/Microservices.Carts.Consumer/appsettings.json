{
  "AppName": "Carts.Consumer",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Bus": {
    "RabbitMQ": {
      "Hostname": "127.0.0.1",
      "Consumer": {
        "IntegrationQueue": "carts-queue"
      }
    }
  },
  "Discovery": {
    "Address": "http://127.0.0.1:8500"
  },
  "EventSourcing": {
    "DatabaseName": "MS-Demo-Carts-db",
    "CollectionName": "Orders"
  },
  "Database": {
    "MongoDb": {
      "DatabaseName": "MS-Demo-Carts-db"
    }
  },
  "AllowedHosts": "*"
}
