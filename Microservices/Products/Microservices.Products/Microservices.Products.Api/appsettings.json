{
  "AppName": "Products.API",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Graylog": {
      "Enabled": true,
      "MinimumLevel": "Error"
    }
  },
  "Bus": {
    "RabbitMQ": {
      "Publisher": {
        "DomainExchange": "products.exchange",
        "IntegrationExchange": "products.exchange"
      }
    }
  },
  "Discovery": {
    "Address": "http://127.0.0.1:8500"
  },
  "Database": {
    "MySQL": {
      "DatabaseName": "MS-Demo-Products-db"
    }
  },
  "AllowedHosts": "*"
}
